let bgColorsArray = ["#e75d7c", "#b16cef", "#53cca4", "#efc84d", "#628ef0", "#184b73", "#883e7f", "#ed048b"];
let container = document.getElementById("bgContainer");

function generate() {
    let lenth = bgColorsArray.length;
    let random = Math.ceil(Math.random() * lenth);
    if (random === lenth) {
        random -= 1;
    }
    container.style.backgroundColor = bgColorsArray[random];

}
